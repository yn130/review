		995
	],
	"time": "30",
	"ingre_list": [
		{
			"ingre_name": "서리태",
			"ingre_count": "2",
			"ingre_unit": "컵"
		},
		{
			"ingre_name": "물",
			"ingre_count": "2",
			"ingre_unit": "컵"
		},
