  <script>
      function checkLoginStatus() {
          fetch('/user/checkLoginStatus')
              .then(response => response.json())
              .then(data => {
                  // 로그인 상태 확인 로직 제거
              });
      }

      document.addEventListener('DOMContentLoaded', () => {
          checkLoginStatus();

          window.addEventListener('popstate', () => {
              checkLoginStatus();
          });
      });
  </script>
</body>
</html>
